import Core'Number
import Test'Assert

SizeZero = \() none | error {
  Assert'Equal(size([none]), 0)
}

SizeOne = \() none | error {
  Assert'Equal(size([none none]), 1)
}

SizeTwo = \() none | error {
  Assert'Equal(size([none none, none]), 2)
}

SizeMany = \() none | error {
  Assert'Equal(size(Number'Sequence(42)), 42)
}
