import Core'Character
import Core'Number

Tag = \(s string) boolean {
  true
}

Key = \(s string) boolean {
  Tag(s)
}

validCharacters = \() [string] {
  [string
    [string
      Character'FromCodePoint(n)
      for n in Number'Range(Character'ToCodePoint("a"), Character'ToCodePoint("z"))
    ],
    "-",
  ]
}
