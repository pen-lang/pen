import 'direction { Direction }

type Position {
  X number
  Y number
}

Move = \(p Position, d Direction) Position {
  if _ = d as direction'Left {
    Position{...p, X: p.X - 1}
  } else if direction'Right {
    Position{...p, X: p.X + 1}
  } else if direction'Up {
    Position{...p, Y: p.Y - 1}
  } else if direction'Down {
    Position{...p, Y: p.Y + 1}
  }
}
