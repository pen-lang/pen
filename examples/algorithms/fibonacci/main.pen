import Core'Number
import Os'File
import Os'Process

main = \(ctx context) none {
  if _ = File'Write(
    ctx.Os,
    File'StdOut(),
    Number'String(fibonacci(40)),
  ) as error {
    Process'Exit(ctx.Os, 1)
  } else {
    none
  }
}

fibonacci = \(n number) number {
  if n <= 0 {
    0
  } else if n == 1 {
    1
  } else {
    fibonacci(n - 1) + fibonacci(n - 2)
  }
}
