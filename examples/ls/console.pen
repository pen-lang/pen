import Core'String
import System'Context { Context }
import System'File

Print = \(ctx Context, s string) none | error {
  File'Write(ctx, File'StdOut(), String'Join([string s, "\n"], ""))?

  none
}

PrintLines = \(ctx Context, ss [string]) none | error {
  if [s, ...ss] = ss {
    Print(ctx, s())?
    PrintLines(ctx, ss)?
  } else {
    none
  }
}
