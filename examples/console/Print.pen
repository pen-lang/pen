import Core'String
import Os'Context { Context }
import Os'File

Line = \(ctx Context, s string) none | error {
  File'Write(ctx, File'StdOut(), String'Join([string s, "\n"], ""))?

  none
}

Lines = \(ctx Context, ss [string]) none | error {
  if [s, ...ss] = ss {
    Line(ctx, s())?
    Lines(ctx, ss)?
  } else {
    none
  }
}
