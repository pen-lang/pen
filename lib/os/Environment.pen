import 'array
import 'Context { Context }

Arguments = \(ctx Context) [string] {
  array'ConvertToStrings(Context'Inner(ctx).GetArguments())
}

Variable = \(ctx Context, name string) string | error {
  r = Context'Inner(ctx).GetEnvironmentVariable(name)

  if r.Error == "" {
    r.Value
  } else {
    error(r.Error)
  }
}
