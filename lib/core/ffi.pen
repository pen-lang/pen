type firstRest {
  ok boolean
  first any
  rest [any]
}

foreign "c" _pen_core_first_rest = \(xs [any]) firstRest {
  if [x, ...xs] = xs {
    firstRest{ok: true, first: x(), rest: xs}
  } else {
    firstRest{ok: false, first: none, rest: [any]}
  }
}

foreign "c" _pen_core_to_string = \(x any) string {
  if x = x as string {
    x
  } else {
    ""
  }
}
