import 'Map'EmptyEntry { EmptyEntry }
import 'Map'Hamt { Hamt }

type Map {
  hamt Hamt
  size number
}

New = \() Map {
  Map{
    hamt: Hamt'New(),
    size: 0,
  }
}

Size = \(map Map) number {
  map.size
}

Get = \(map Map, key string, default any) any {
  v = Hamt'Get(map.hamt, key)

  if _ = v as EmptyEntry {
    default
  } else {
    v
  }
}
