import Test'Assert
import 'Parse { Parse }
import 'Value

ParseNull = \() none | error {
  Assert'True(Parse("null")? == Value'New(none))
}

ParseTrue = \() none | error {
  Assert'True(Parse("true")? == Value'New(true))
}

ParseFalse = \() none | error {
  Assert'True(Parse("false")? == Value'New(false))
}
